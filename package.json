{
  "name": "edit-anywhere",
  "version": "0.0.1",
  "scripts": {
    "generate": "node -e \"require('./index.js').renderIndex()\"",
    "update": "git add -A && git commit -m \"auto-update\" && git push",
    "start": "node -e \"require('./index.js').startServer()\"",
    "debug": "node --inspect-brk -e \"require('./index.js').startServer()\"",
    "build": "node-gyp rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/briancullinan2/edit-anywhere.git"
  },
  "author": "Brian Cullinan",
  "license": "Unlicense",
  "readmeFilename": "README.md",
  "dependencies": {
    "better-sqlite3": "^7.6.2",
    "d3-node": "^0.2.0",
    "express": "^4.18.1",
    "google-auth-library": "^8.7.0",
    "googleapis": "^110.0.0",
    "jsdom": "^20.0.3",
    "JSONStream": "^1.3.5",
    "moment": "^2.29.4",
    "mustache": "^4.2.0",
    "node-addon-api": "^5.0.0",
    "node-gyp": "^9.1.0",
    "puppeteer": "^18.2.1",
    "remarkable": "^2.0.1",
    "wgxpath": "^1.2.0"
  }
}
